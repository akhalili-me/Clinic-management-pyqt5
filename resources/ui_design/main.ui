      <property name="styleSheet">
       <string notr="true">/* Basic ListWidget styling */
QListWidget {
    background-color: #f8f9fa; /* Light grey background */
    border: 1px solid #dee2e6; /* Border color */
    border-radius: 5px; /* Rounded corners */
    padding: 5px; /* Padding inside the list */
    outline: 0; /* Remove focus indicator */
}

/* Item styling */
QListWidget::item {
    padding: 10px 5px; /* Padding for each item */
    margin: 5px 0; /* Margin between items */
    background-color: #ffffff; /* White background for items */
    border-radius: 5px; /* Rounded corners for items */
    color: #343a40; /* Dark grey text color */
    outline: 0; /* Remove focus indicator */
}

/* Hover effect for items */
QListWidget::item:hover {
    background-color: #e9ecef; /* Light grey background on hover */
}

/* Selected item styling */
QListWidget::item:selected {
    background-color: #0050a5;
	color: white;
}

/* Scroll bar styling */
QScrollBar:vertical {
    background: #f8f9fa; /* Light grey background for the scroll bar */
    width: 12px; /* Width of the vertical scroll bar */
    margin: 22px 0 22px 0; /* Margin to fit the top and bottom arrows */
    border: 1px solid #dee2e6; /* Border color */
    border-radius: 5px; /* Rounded corners */
}

QScrollBar::handle:vertical {
    background: #ced4da; /* Medium grey handle */
    min-height: 20px; /* Minimum height of the handle */
    border-radius: 5px; /* Rounded corners */
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    background: #ced4da; /* Medium grey for arrows */
    height: 20px;
    subcontrol-origin: margin;
    subcontrol-position: top;
    border-radius: 5px; /* Rounded corners */
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}
</string>
