    <string notr="true">/* Basic LineEdit styling */
QLineEdit {
    background-color: #ffffff; /* White background */
    border: 1px solid #dee2e6; /* Light grey border */
    border-radius: 5px; /* Rounded corners */
    color: #343a40; /* Dark grey text color */
    outline: none; /* Remove default outline */
}

/* Hover effect */
QLineEdit:hover {
    border: 1px solid #007bff; /* Blue border on hover */
}

/* Focus effect */
QLineEdit:focus {
    border: 1px solid #007bff; /* Blue border on focus */
}

/* Disabled state */
QLineEdit:disabled {
    background-color: #e9ecef; /* Light grey background when disabled */
    color: #6c757d; /* Grey text color when disabled */
}

